extends layout

block content
  h1= title
  p Welcome to #{title} 
  br       
  - var imageName= ['1.jpg','5.jpg','3.jpg','4.jpg','2.jpg']   
     -for(let i=0;i<3;i++){
        - let r=Math.floor(Math.random()*5)
            img(src= `/images/${imageName[r]}` alt="image", height="300", width="300")
        -}